<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
 "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
 
<mapper namespace="com.healthStore.post.dao.PostDAO">
   <insert id="insertPost" parameterType="map">
      INSERT INTO `post`
      (
           `productId`
           ,`productName`
           ,`price`
           ,`imagePath`
           ,`createdAt`
           ,`updatedAt`
      )
      VALUES
      (
           #{productId}
           ,#{productName}
           ,#{price}
           ,#{imagePath}
           ,NOW()
           ,NOW()
      )
   </insert>
</mapper>